<!DOCTYPE html>
<html>
<head>
  <title>Editar</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet"
  integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/css/bootstrap-datepicker.min.css">
  <link rel="stylesheet" href="https://code.jquery.com/ui/1.13.0/themes/base/jquery-ui.css">

</head>
<body>
    <h1>Editar datos</h1>
    <form action="/edit<%= tipo %>/<%= id%>/?accessToken=<%= token %>" method="POST">
      <% switch (tipo) {
        case 'cuenta':
          %>
          <div class="mb-3 p-3">
        <label for="id" class="form-label">Id</label>
        <input type="text" class="form-control" id="id" name="id" tabindex="1" value="<%=id%>" readonly>
      </div>
          <div class="mb-3 p-3">
        <label for="acreditador" class="form-label">Acreditador</label>
        <input type="text" class="form-control" id="acreditador" name="acreditador" tabindex="1" required>
      </div>
      <div class="mb-3 p-3">
        <label for="saldo" class="form-label">Saldo</label>
        <input type="text" class="form-control" id="saldo" name="saldo" tabindex="1" required>
      </div>
      <div class="mb-3 p-3">
        <label for="estado" class="form-label">Estado</label>
        <input type="text" class="form-control" id="estado" name="estado" tabindex="1" required>
      </div>
      <div class="d-flex justify-content-between">
      <div class="m-3">
        <button type="submit" class="btn btn-primary " tabindex="5">Editar</button>
      </div>
     
    </form>
  </div>
  </div>
          <% break;
        case 'clientes':
          %>
          <div class="mb-3 p-3">
        <label for="id" class="form-label">Id</label>
        <input type="text" class="form-control" id="id" name="id" tabindex="1" value="<%=id%>" readonly>
      </div>
          <div class="mb-3 p-3">
        <label for="Nombre" class="form-label">Nombre</label>
        <input type="text" class="form-control" id="Nombre" name="Nombre" tabindex="1" required>
      </div>
      <div class="mb-3 p-3">
        <label for="Apellido" class="form-label">Apellido</label>
        <input type="text" class="form-control" id="Apellido" name="Apellido" tabindex="1" required>
      </div>
      <div class="mb-3 p-3">
        <label for="Pais" class="form-label">Pais</label>
        <input type="text" class="form-control" id="Pais" name="Pais" tabindex="1" required>
      </div>
      <div class="d-flex justify-content-between">
      <div class="m-3">
        <button type="submit" class="btn btn-primary " tabindex="5">Editar</button>
      </div>
          <% break;
        case 'productos':
          %>
          <div class="mb-3 p-3">
        <label for="id" class="form-label">Id</label>
        <input type="text" class="form-control" id="id" name="id" tabindex="1" value="<%=id%>" readonly>
      </div>
          <div class="mb-3 p-3">
        <label for="producto" class="form-label">Producto</label>
        <input type="text" class="form-control" id="producto" name="producto" tabindex="1" required>
      </div>
      <div class="mb-3 p-3">
        <label for="cantidad" class="form-label">Cantidad</label>
        <input type="text" class="form-control" id="cantidad" name="cantidad" tabindex="1" required>
      </div>
      <div class="mb-3 p-3">
        <label for="precio" class="form-label">Precio</label>
        <input type="text" class="form-control" id="precio" name="precio" tabindex="1" required>
      </div>
      <div class="d-flex justify-content-between">
      <div class="m-3">
        <button type="submit" class="btn btn-primary " tabindex="5">Editar</button>
      </div>
     
    </form>
   </div>
  </div>
      <% break;

      case 'repartidores':
          %>
          <div class="mb-3 p-3">
        <label for="id" class="form-label">Id</label>
        <input type="text" class="form-control" id="id" name="id" tabindex="1" value="<%=id%>" readonly>
      </div>
          <div class="mb-3 p-3">
        <label for="nombre" class="form-label">Nombre</label>
        <input type="text" class="form-control" id="nombre" name="nombre" tabindex="1" required>
      </div>
      <div class="mb-3 p-3">
        <label for="apellido" class="form-label">Apellido</label>
        <input type="text" class="form-control" id="apellido" name="apellido" tabindex="1" required>
      </div>
      <div class="mb-3 p-3">
        <label for="vehiculo" class="form-label">Vehiculo</label>
        <input type="text" class="form-control" id="vehiculo" name="vehiculo" tabindex="1" required>
      </div>
      <div class="mb-3 p-3">
        <label for="sector" class="form-label">Sector</label>
        <input type="text" class="form-control" id="sector" name="sector" tabindex="1" required>
      </div>
      <div class="d-flex justify-content-between">
      <div class="m-3">
        <button type="submit" class="btn btn-primary " tabindex="5">Editar</button>
      </div>
     
    </form>
   </div>
  </div>
      <% break;

      case 'facturas':
          %>
          <div class="mb-3 p-3">
        <label for="id" class="form-label">Id</label>
        <input type="text" class="form-control" id="id" name="id" tabindex="1" value="<%=id%>" readonly>
      </div>
          <div class="mb-3 p-3">
        <label for="nombre" class="form-label">Nombre</label>
        <input type="text" class="form-control" id="nombre" name="nombre" tabindex="1" required>
      </div>
      <div class="mb-3 p-3">
        <label for="motivo" class="form-label">Motivo</label>
        <input type="text" class="form-control" id="motivo" name="motivo" tabindex="1" required>
      </div>
      <div class="mb-3 p-3">
        <label for="fecha" class="form-label">Fecha</label>
        <input type="text" class="form-control datepicker" id="fecha" name="fecha" tabindex="1" required>
      </div>
      <div class="mb-3 p-3">
        <label for="valor" class="form-label">Valor</label>
        <input type="text" class="form-control" id="valor" name="valor" tabindex="1" required>
      </div>
      <div class="d-flex justify-content-between">
      <div class="m-3">
        <button type="submit" class="btn btn-primary " tabindex="5">Editar</button>
      </div>
     
    </form>
   </div>
  </div>
      <% break;
      
    default:
      %>
      <h2>Tipo no v√°lido</h2>
      <% break;
  } %>
</form>
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script src="https://code.jquery.com/ui/1.13.0/jquery-ui.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-ygbV9kiqUc6oa4msXn9868pTtWMgiQaeYH7/t7LECLbyPA2x65Kgf80OJFdroafW"
    crossorigin="anonymous"></script>
    <script>
    $(document).ready(function() {
      $('.datepicker').datepicker({
        dateFormat: 'dd-mm-yy'
      });
    });
  </script>
    </body>

</html>

